{
    "name": "portal-monorepo",
    "private": true,
    "packageManager": "pnpm@9.0.0",
    "engines": {
      "node": ">=22.0.0"
    },
    "devDependencies": {
      "turbo": "^2.0.0"
    },
    "scripts": {    
        "dev:web": "turbo run dev:web --filter=@apps/portal --parallel",
        "build:web": "turbo run build --filter=@apps/portal",
        "lint:web": "turbo run lint --filter=@apps/portal",
        "type-check:web": "turbo run type-check --filter=@apps/portal"
    }
}
  